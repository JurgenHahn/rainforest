<h1>Products#index</h1>
<p>Find me in app/views/products/index.html.erb</p>

<%= link_to "New Product", new_product_path %>

<% @products.each do |product| %>

<%= link_to(product) do %>
<div class="thumbnail">
<img src="<%=product.image_url%>">

<%= product.name %>

<%= product.price_in_cents %>
<%= product.description %>
</div>
<% end %>

<% end %>
